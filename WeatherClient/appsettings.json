{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AmbientData": {
    "AggregationInterval": 1
  },
  "RateLimiter": {
    "Global": {
      "MaxRequests": 2,
      "TimeWindowInSeconds": 5
    },
    "Clients": {
      "127.0.0.2": {
        "MaxRequests": 2,
        "TimeWindowInSeconds": 60
      }
    }
  },
  "AllowedHosts": "*"
}
